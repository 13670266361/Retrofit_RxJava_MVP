<?xml version="1.0" encoding="utf-8"?>
<com.example.y.mvp.utils.theme.widget.ThemeLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/main_bg"
    android:orientation="vertical">


    <include layout="@layout/toolbar" />

    <com.example.y.mvp.utils.theme.widget.ThemeFrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?attr/main_bg">

        <android.support.v4.widget.SwipeRefreshLayout
            android:id="@+id/srf_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <WebView
                android:id="@+id/webView"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </android.support.v4.widget.SwipeRefreshLayout>
    </com.example.y.mvp.utils.theme.widget.ThemeFrameLayout>
</com.example.y.mvp.utils.theme.widget.ThemeLinearLayout>